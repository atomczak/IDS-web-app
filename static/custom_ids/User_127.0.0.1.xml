    <specification name="Fire requirement specification" necessity="required">
        <applicability>
            <entity>
                <name>
                    <simpleValue>IfcWall</simpleValue>
                </name>
            </entity>
        </applicability>
        <requirements>
            <material location="any">
                <value>
                    <simpleValue>TIMBERs</simpleValue>
